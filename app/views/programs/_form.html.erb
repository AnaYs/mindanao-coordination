<%= form_for(@program) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, required: true %>

  <%= f.label :main_office %>
  <%= f.text_field :main_office, required: true %>

  <%= f.label :area_of_work %>
  <%= f.text_field :area_of_work, required: true %>

  <%= f.label :organisation %>
  <%= f.text_field :organisation %>

  <%= f.submit %>
<% end %>
